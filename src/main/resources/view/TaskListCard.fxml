<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox id="cardPane" fx:id="cardPane" styleClass="list-view-card-pane" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
    <children>
               <Label fx:id="id" alignment="CENTER" styleClass="cell_big_label" text="\$index" textAlignment="CENTER" wrapText="true" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets bottom="3.0" left="5.0" top="3.0" />
         </HBox.margin>
      </Label>
      <GridPane HBox.hgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" vgrow="ALWAYS" />
          <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" vgrow="ALWAYS" />
        </rowConstraints>
         <children>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <HBox VBox.vgrow="ALWAYS">
                     <children>
                                 <Label fx:id="detail" alignment="TOP_LEFT" ellipsisString="" styleClass="cell_big_label" stylesheets="@../CSS/Labels.css" text="\$task" textOverrun="CLIP" wrapText="true" HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets bottom="3.0" left="2.0" right="2.0" top="3.0" />
                           </HBox.margin>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </VBox>
            <HBox prefHeight="48.0" prefWidth="313.0" GridPane.rowIndex="1">
               <children>
                  <Region prefWidth="20.0" />
                  <Rectangle fx:id="status" arcHeight="5.0" arcWidth="5.0" fill="#ff1f1f" height="50.0" stroke="BLACK" strokeType="INSIDE" width="80.0">
                     <HBox.margin>
                        <Insets bottom="3.0" />
                     </HBox.margin>
                  </Rectangle>
                  <VBox fillWidth="false">
                     <children>
                        <HBox>
                           <children>
                              <Region prefWidth="40.0" />
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Start " underline="true">
                                 <font>
                                    <Font name="Helvetica Neue UltraLight" size="10.0" />
                                 </font>
                              </Text>
                              <Label fx:id="sd" alignment="CENTER" prefWidth="85.0" text="10/19/2016" />
                              <Label fx:id="st" alignment="CENTER" prefWidth="45.0" text="2359" />
                              <Region prefWidth="65.0" HBox.hgrow="ALWAYS" />
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Tags: ">
                                 <font>
                                    <Font name="Helvetica Neue UltraLight" size="11.0" />
                                 </font>
                              </Text>
                                      <Label fx:id="tags" alignment="CENTER" styleClass="cell_small_label" text="\$tags" />
                           </children>
                        </HBox>
                        <HBox>
                           <children>
                              <Region prefWidth="40.0" />
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="End " underline="true">
                                 <font>
                                    <Font name="Helvetica Neue UltraLight" size="10.0" />
                                 </font>
                              </Text>
                                    <Label fx:id="dbd" alignment="CENTER" prefWidth="85.0" styleClass="cell_small_label" text="10/19/2016" />
                                    <Label fx:id="dbt" alignment="CENTER" prefWidth="45.0" styleClass="cell_small_label" text="2359" />
                              <Region prefWidth="65.0" HBox.hgrow="ALWAYS" />
                              <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Priority: ">
                                 <font>
                                    <Font name="Helvetica Neue UltraLight" size="11.0" />
                                 </font>
                              </Text>
                                    <Label fx:id="priority" alignment="CENTER" prefWidth="49.0" styleClass="cell_small_label" text="medium" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </GridPane>
    </children>
</HBox>
