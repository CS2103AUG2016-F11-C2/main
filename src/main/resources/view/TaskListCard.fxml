<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>

<HBox id="cardPane" fx:id="cardPane" prefHeight="30.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <HBox prefHeight="20.0" prefWidth="773.0">
         <children>
                     <Label fx:id="id" alignment="BOTTOM_CENTER" prefHeight="30.0" prefWidth="30.0" styleClass="cell_big_label" text="\$index" />
                     <Label fx:id="detail" maxWidth="500.0" prefHeight="30.0" prefWidth="500.0" styleClass="cell_big_label" text="\$task" />
                    <Label fx:id="tags" alignment="CENTER" prefHeight="30.0" prefWidth="41.0" styleClass="cell_small_label" text="\$tags" />
                  <Label fx:id="dbt" alignment="CENTER" prefHeight="30.0" prefWidth="41.0" styleClass="cell_small_label" text="\$duetime" />
                  <Label fx:id="dbd" alignment="CENTER" prefHeight="30.0" prefWidth="60.0" styleClass="cell_small_label" text="\$duedate" />
                  <Label fx:id="priority" alignment="CENTER" prefHeight="30.0" prefWidth="95.0" styleClass="cell_small_label" text="priority" />
         </children>
      </HBox>
    </children>
</HBox>
